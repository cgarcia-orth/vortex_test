	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.3213 ] ; then
		printf "%b\0" "bbnote $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.3213
	else
		echo "NOTE: $*"
	fi
}

bbfatal() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.3213 ] ; then
		printf "%b\0" "bbfatal $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.3213
	else
		echo "ERROR: $*"
	fi
	exit 1
}

