base_do_compile() {
	if [ -e Makefile -o -e makefile -o -e GNUmakefile ]; then
		oe_runmake || die "make failed"
	else
		bbnote "nothing to compile"
	fi
}

base_do_configure() {
	if [ -n "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate" -a -e "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate" ]; then
		if [ "`cat /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate`" != "${BB_TASKHASH}" ]; then
			cd /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build
			if [ "0" != "1" -a \( -e Makefile -o -e makefile -o -e GNUmakefile \) ]; then
				oe_runmake clean
			fi
			# -ignore_readdir_race does not work correctly with -delete;
			# use xargs to avoid spurious build failures
			find /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build -ignore_readdir_race -name \*.la -type f -print0 | xargs -0 rm -f
		fi
	fi
	if [ -n "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate" ]; then
		mkdir -p `dirname /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate`
		echo ${BB_TASKHASH} > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/configure.sstate
	fi
}

base_do_install() {
	:
}

base_do_package() {
	:
}

bbdebug() {
	USAGE='Usage: bbdebug [123] "message"'
	if [ $# -lt 2 ]; then
		bbfatal "$USAGE"
	fi

	# Strip off the debug level and ensure it is an integer
	DBGLVL=$1; shift
	NONDIGITS=$(echo "$DBGLVL" | tr -d "[:digit:]")
	if [ "$NONDIGITS" ]; then
		bbfatal "$USAGE"
	fi

	# All debug output is printed to the logs
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbdebug $DBGLVL $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "DEBUG: $*"
	fi
}

bberror() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bberror $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "ERROR: $*"
	fi
}

bbfatal() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbfatal $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "ERROR: $*"
	fi
	exit 1
}

bbfatal_log() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbfatal_log $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "ERROR: $*"
	fi
	exit 1
}

bbnote() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbnote $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "NOTE: $*"
	fi
}

bbplain() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbplain $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "$*"
	fi
}

bbwarn() {
	if [ -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124 ] ; then
		printf "%b\0" "bbwarn $*" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/temp/fifo.19124
	else
		echo "WARNING: $*"
	fi
}

buildhistory_commit() {
	if [ ! -d /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory ] ; then
		# Code above that creates this dir never executed, so there can't be anything to commit
		return
	fi

	# Create a machine-readable list of metadata revisions for each layer
	cat > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/metadata-revs <<END
meta              = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-poky         = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-yocto-bsp    = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-vortex-var-som-mx6 = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-vortex-apps  = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale    = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale-3rdparty = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale-distro = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-qt5          = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-oe           = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-multimedia   = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-gnome        = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-python       = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-networking   = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-filesystems  = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-swupdate     = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-variscite-fslc = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-influx       = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-vortex-var-som-mx6 = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-vortex-apps  = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale    = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale-3rdparty = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-freescale-distro = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-qt5          = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-oe           = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-multimedia   = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-gnome        = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-python       = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-networking   = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-filesystems  = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-swupdate     = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-variscite-fslc = master:edf3f6aee5d1292dca560308d43da2459c134b1d
meta-influx       = master:edf3f6aee5d1292dca560308d43da2459c134b1d
END

	( cd /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/
		# Initialise the repo if necessary
		if [ ! -e .git ] ; then
			git init -q
		else
			git tag -f build-minus-3 build-minus-2 > /dev/null 2>&1 || true
			git tag -f build-minus-2 build-minus-1 > /dev/null 2>&1 || true
			git tag -f build-minus-1 > /dev/null 2>&1 || true
		fi

		check_git_config

		# Check if there are new/changed files to commit (other than metadata-revs)
		repostatus=`git status --porcelain | grep -v " metadata-revs$"`
		HOSTNAME=`hostname 2>/dev/null || echo unknown`
		CMDLINE="bitbake vortex-core -e"
		if [ "$repostatus" != "" ] ; then
			git add -A .
			# porcelain output looks like "?? packages/foo/bar"
			# Ensure we commit metadata-revs with the first commit
			buildhistory_single_commit "$CMDLINE" "$HOSTNAME" dummy
			git gc --auto --quiet
		else
			buildhistory_single_commit "$CMDLINE" "$HOSTNAME"
		fi
		if [ "" != "" ] ; then
			git push -q 
		fi) || true
}

buildhistory_get_image_installed() {
	# Anything requiring the use of the packaging system should be done in here
	# in case the packaging files are going to be removed for this image

	if [ "1" = "0" ] ; then
		return
	fi

	buildhistory_get_installed /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core
}

buildhistory_get_imageinfo() {
	if [ "1" = "0" ] ; then
		return
	fi

        mkdir -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core
	buildhistory_list_files /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/rootfs /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/files-in-image.txt

	# Collect files requested in BUILDHISTORY_IMAGE_FILES
	rm -rf /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-files
	for f in /etc/passwd /etc/group; do
		if [ -f /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/rootfs/$f ] ; then
			mkdir -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-files/`dirname $f`
			cp /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/rootfs/$f /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-files/$f
		fi
	done

	# Record some machine-readable meta-information about the image
	printf ""  > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-info.txt
	cat >> /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-info.txt <<END

END
	imagesize=`du -ks /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/rootfs | awk '{ print $1 }'`
	echo "IMAGESIZE = $imagesize" >> /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/image-info.txt

	# Add some configuration information
	echo "var-som-mx6: vortex-core configured for vortex-fslc-x11 2.6.0" > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/build-id.txt

	cat >> /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/images/var_som_mx6/glibc/vortex-core/build-id.txt <<END

END
}

buildhistory_get_installed() {
	mkdir -p $1

	# Get list of installed packages
	pkgcache="$1/installed-packages.tmp"
	cat /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/bh_installed_pkgs.txt | sort > $pkgcache && rm /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/bh_installed_pkgs.txt

	cat $pkgcache | awk '{ print $1 }' > $1/installed-package-names.txt
	if [ -s $pkgcache ] ; then
		cat $pkgcache | awk '{ print $2 }' | xargs -n1 basename > $1/installed-packages.txt
	else
		printf "" > $1/installed-packages.txt
	fi

	# Produce dependency graph
	# First, quote each name to handle characters that cause issues for dot
	sed 's:\([^| ]*\):"\1":g' /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/bh_installed_pkgs_deps.txt > $1/depends.tmp &&
		rm /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/bh_installed_pkgs_deps.txt
	# Remove lines with rpmlib(...) and config(...) dependencies, change the
	# delimiter from pipe to "->", set the style for recommend lines and
	# turn versioned dependencies into edge labels.
	sed -i -e '/rpmlib(/d' \
	       -e '/config(/d' \
	       -e 's:|: -> :' \
	       -e 's:"\[REC\]":[style=dotted]:' \
	       -e 's:"\([<>=]\+\)" "\([^"]*\)":[label="\1 \2"]:' \
		$1/depends.tmp
	# Add header, sorted and de-duped contents and footer and then delete the temp file
	printf "digraph depends {\n    node [shape=plaintext]\n" > $1/depends.dot
	cat $1/depends.tmp | sort -u >> $1/depends.dot
	echo "}" >>  $1/depends.dot
	rm $1/depends.tmp

	# Produce installed package sizes list
	oe-pkgdata-util -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/pkgdata/var-som-mx6 read-value "PKGSIZE" -n -f $pkgcache > $1/installed-package-sizes.tmp
	cat $1/installed-package-sizes.tmp | awk '{print $2 "\tKiB\t" $1}' | sort -n -r > $1/installed-package-sizes.txt
	rm $1/installed-package-sizes.tmp

	# We're now done with the cache, delete it
	rm $pkgcache

	if [ "$2" != "sdk" ] ; then
		# Produce some cut-down graphs (for readability)
		grep -v kernel-image $1/depends.dot | grep -v kernel-3 | grep -v kernel-4 > $1/depends-nokernel.dot
		grep -v libc6 $1/depends-nokernel.dot | grep -v libgcc > $1/depends-nokernel-nolibc.dot
		grep -v update- $1/depends-nokernel-nolibc.dot > $1/depends-nokernel-nolibc-noupdate.dot
		grep -v kernel-module $1/depends-nokernel-nolibc-noupdate.dot > $1/depends-nokernel-nolibc-noupdate-nomodules.dot
	fi

	# add complementary package information
	if [ -e /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/complementary_pkgs.txt ]; then
		cp /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/complementary_pkgs.txt $1
	fi
}

buildhistory_get_sdk_installed() {
	# Anything requiring the use of the packaging system should be done in here
	# in case the packaging files are going to be removed for this SDK

	if [ "1" = "0" ] ; then
		return
	fi

	buildhistory_get_installed /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/$1 sdk
}

buildhistory_get_sdk_installed_host() {
	buildhistory_get_sdk_installed host
}

buildhistory_get_sdk_installed_target() {
	buildhistory_get_sdk_installed target
}

buildhistory_get_sdkinfo() {
	if [ "1" = "0" ] ; then
		return
	fi

	buildhistory_list_files ${SDK_OUTPUT} /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/files-in-sdk.txt

	# Collect files requested in BUILDHISTORY_SDK_FILES
	rm -rf /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-files
	for f in conf/local.conf conf/bblayers.conf conf/auto.conf conf/locked-sigs.inc conf/devtool.conf; do
		if [ -f ${SDK_OUTPUT}//opt/vortex-fslc-x11/2.6.2/$f ] ; then
			mkdir -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-files/`dirname $f`
			cp ${SDK_OUTPUT}//opt/vortex-fslc-x11/2.6.2/$f /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-files/$f
		fi
	done

	# Record some machine-readable meta-information about the SDK
	printf ""  > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-info.txt
	cat >> /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-info.txt <<END

END
	sdksize=`du -ks ${SDK_OUTPUT} | awk '{ print $1 }'`
	echo "SDKSIZE = $sdksize" >> /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/sdk/vortex-fslc-x11-glibc-x86_64-vortex-core-armv7at2hf-neon${SDK_EXT}/vortex-core/sdk-info.txt
}

buildhistory_list_files() {
	# List the files in the specified directory, but exclude date/time etc.
	# This awk script is somewhat messy, but handles where the size is not printed for device files under pseudo
	if [ "$3" = "fakeroot" ] ; then
		( cd $1 && PSEUDO_PREFIX=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/sysroots-components/x86_64/pseudo-native/usr PSEUDO_LOCALSTATEDIR=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/pseudo/ PSEUDO_PASSWD=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot:/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/sysroots-components/x86_64/pseudo-native PSEUDO_NOSYMLINKEXP=1 PSEUDO_DISABLED=0 /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/sysroots-components/x86_64/pseudo-native/usr/bin/pseudo find . ! -path . -printf "%M %-10u %-10g %10s %p -> %l\n" | sort -k5 | sed 's/ * -> $//' > $2 )
	else
		( cd $1 && find . ! -path . -printf "%M %-10u %-10g %10s %p -> %l\n" | sort -k5 | sed 's/ * -> $//' > $2 )
	fi
}

buildhistory_list_pkg_files() {
	# Create individual files-in-package for each recipe's package
	for pkgdir in $(find /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/packages-split/* -maxdepth 0 -type d); do
		pkgname=$(basename $pkgdir)
		outfolder="/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/packages/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/$pkgname"
		outfile="$outfolder/files-in-package.txt"
		# Make sure the output folder exists so we can create the file
		if [ ! -d $outfolder ] ; then
			bbdebug 2 "Folder $outfolder does not exist, file $outfile not created"
			continue
		fi
		buildhistory_list_files $pkgdir $outfile fakeroot
	done
}

buildhistory_single_commit() {
	if [ "$3" = "" ] ; then
		commitopts="/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/ --allow-empty"
		shortlogprefix="No changes: "
	else
		commitopts=""
		shortlogprefix=""
	fi
	if [ "${BUILDHISTORY_BUILD_FAILURES}" = "0" ] ; then
		result="succeeded"
	else
		result="failed"
	fi
	case ${BUILDHISTORY_BUILD_INTERRUPTED} in
		1)
			result="$result (interrupted)"
			;;
		2)
			result="$result (force interrupted)"
			;;
	esac
	commitmsgfile=`mktemp`
	cat > $commitmsgfile << END
${shortlogprefix}Build ${BUILDNAME} of vortex-fslc-x11 2.6.0 for machine var-som-mx6 on $2

cmd: $1

result: $result

metadata revisions:
END
	cat /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/buildhistory/metadata-revs >> $commitmsgfile
	git commit $commitopts -F $commitmsgfile --author "buildhistory <buildhistory@vortex-fslc-x11>" > /dev/null
	rm $commitmsgfile
}

check_git_config() {
	if ! git config user.email > /dev/null ; then
		git config --local user.email "oe.patch@oe"
	fi
	if ! git config user.name > /dev/null ; then
		git config --local user.name "OpenEmbedded"
	fi
}

cmake_do_compile() {
	cmake_runcmake_build --target all
}

cmake_do_configure() {
	if [ "${OECMAKE_BUILDPATH}" ]; then
		bbnote "cmake.bbclass no longer uses OECMAKE_BUILDPATH.  The default behaviour is now out-of-tree builds with B=WORKDIR/build."
	fi

	if [ "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0" != "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build" ]; then
		rm -rf /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build
		mkdir -p /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build
		cd /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build
	else
		find /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build -name CMakeFiles -or -name Makefile -or -name cmake_install.cmake -or -name CMakeCache.txt -delete
	fi

	# Just like autotools cmake can use a site file to cache result that need generated binaries to run
	if [ -e /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/site-file.cmake ] ; then
		oecmake_sitefile="-C /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/site-file.cmake"
	else
		oecmake_sitefile=
	fi

	cmake \
	  -G Ninja -DCMAKE_MAKE_PROGRAM=ninja \
	  $oecmake_sitefile \
	  /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/VortexCore/src \
	  -DCMAKE_INSTALL_PREFIX:PATH=/usr \
	  -DCMAKE_INSTALL_BINDIR:PATH=bin \
	  -DCMAKE_INSTALL_SBINDIR:PATH=sbin \
	  -DCMAKE_INSTALL_LIBEXECDIR:PATH=libexec \
	  -DCMAKE_INSTALL_SYSCONFDIR:PATH=/etc \
	  -DCMAKE_INSTALL_SHAREDSTATEDIR:PATH=../com \
	  -DCMAKE_INSTALL_LOCALSTATEDIR:PATH=/var \
	  -DCMAKE_INSTALL_LIBDIR:PATH=lib \
	  -DCMAKE_INSTALL_INCLUDEDIR:PATH=include \
	  -DCMAKE_INSTALL_DATAROOTDIR:PATH=share \
	  -DCMAKE_INSTALL_SO_NO_EXE=0 \
	  -DCMAKE_TOOLCHAIN_FILE=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/toolchain.cmake \
	  -DCMAKE_VERBOSE_MAKEFILE=1 \
	  -DCMAKE_NO_SYSTEM_FROM_IMPORTED=1 \
	   ${PACKAGECONFIG_CONFARGS} \
	  -Wno-dev
}

cmake_do_generate_toolchain_file() {
	if [ "x86_64-linux" = "arm-fslc-linux-gnueabi" ]; then
		cmake_crosscompiling="set( CMAKE_CROSSCOMPILING FALSE )"
	fi
	cat > /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/toolchain.cmake <<EOF
# CMake system name must be something like "Linux".
# This is important for cross-compiling.
$cmake_crosscompiling
set( CMAKE_SYSTEM_NAME `echo linux-gnueabi | sed -e 's/^./\u&/' -e 's/^\(Linux\).*/\1/'` )
set( CMAKE_SYSTEM_PROCESSOR arm )
set( CMAKE_C_COMPILER `echo arm-fslc-linux-gnueabi-gcc  -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot | sed 's/^\([^ ]*\).*/\1/'` )
set( CMAKE_CXX_COMPILER `echo arm-fslc-linux-gnueabi-g++  -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot | sed 's/^\([^ ]*\).*/\1/'` )
set( CMAKE_ASM_COMPILER `echo arm-fslc-linux-gnueabi-gcc  -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot | sed 's/^\([^ ]*\).*/\1/'` )
set( CMAKE_AR arm-fslc-linux-gnueabi-ar CACHE FILEPATH "Archiver" )
set( CMAKE_C_FLAGS " -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot  -O2 -pipe -g -feliminate-unused-debug-types -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0=/usr/src/debug/vortex-core/1.0.5-r0 -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot= -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot-native=   -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot" CACHE STRING "CFLAGS" )
set( CMAKE_CXX_FLAGS " -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot  -O2 -pipe -g -feliminate-unused-debug-types -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0=/usr/src/debug/vortex-core/1.0.5-r0 -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot= -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot-native=  -fvisibility-inlines-hidden  -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot" CACHE STRING "CXXFLAGS" )
set( CMAKE_ASM_FLAGS " -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot  -O2 -pipe -g -feliminate-unused-debug-types -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0=/usr/src/debug/vortex-core/1.0.5-r0 -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot= -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot-native=   -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot" CACHE STRING "ASM FLAGS" )
set( CMAKE_C_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional CFLAGS for release" )
set( CMAKE_CXX_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional CXXFLAGS for release" )
set( CMAKE_ASM_FLAGS_RELEASE "-DNDEBUG" CACHE STRING "Additional ASM FLAGS for release" )
set( CMAKE_C_LINK_FLAGS " -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot  -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed -fstack-protector-strong -Wl,-z,relro,-z,now" CACHE STRING "LDFLAGS" )
set( CMAKE_CXX_LINK_FLAGS " -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot  -O2 -pipe -g -feliminate-unused-debug-types -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0=/usr/src/debug/vortex-core/1.0.5-r0 -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot= -fdebug-prefix-map=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot-native=  -fvisibility-inlines-hidden  -march=armv7-a -mthumb -mfpu=neon -mfloat-abi=hard -fstack-protector-strong  -D_FORTIFY_SOURCE=2 -Wformat -Wformat-security -Werror=format-security  --sysroot=/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot -Wl,-O1 -Wl,--hash-style=gnu -Wl,--as-needed -fstack-protector-strong -Wl,-z,relro,-z,now" CACHE STRING "LDFLAGS" )

# only search in the paths provided so cmake doesnt pick
# up libraries and tools from the native build machine
set( CMAKE_FIND_ROOT_PATH /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot-native ${CROSS_DIR}   ${EXTERNAL_TOOLCHAIN})
set( CMAKE_FIND_ROOT_PATH_MODE_PACKAGE ONLY )
set( CMAKE_FIND_ROOT_PATH_MODE_PROGRAM ONLY )
set( CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY )
set( CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY )

# Use qt.conf settings
set( ENV{QT_CONF_PATH} /home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/qt.conf )

# We need to set the rpath to the correct directory as cmake does not provide any
# directory as rpath by default
set( CMAKE_INSTALL_RPATH  )

# Use native cmake modules
list(APPEND CMAKE_MODULE_PATH "/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/recipe-sysroot/usr/share/cmake/Modules/")

# add for non /usr/lib libdir, e.g. /usr/lib64
set( CMAKE_LIBRARY_PATH /usr/lib /lib)

EOF
}

cmake_do_install() {
	DESTDIR='/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/image' cmake_runcmake_build --target install
}

cmake_runcmake_build() {
	bbnote ${DESTDIR:+DESTDIR=${DESTDIR} }VERBOSE=1 cmake --build '/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build' "$@" -- 
	eval ${DESTDIR:+DESTDIR=${DESTDIR} }VERBOSE=1 cmake --build '/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/build' "$@" -- 
}

create_cmdline_wrapper() {
	# Create a wrapper script where commandline options are needed
	#
	# These are useful to work around relocation issues, by passing extra options
	# to a program
	#
	# Usage: create_cmdline_wrapper FILENAME <extra-options>

	cmd=$1
	shift

	echo "Generating wrapper script for $cmd"

	mv $cmd $cmd.real
	cmdname=`basename $cmd`
	dirname=`dirname $cmd`
	cmdoptions=$@
	if [ "" != "" ]; then
		relpath=`python3 -c "import os; print(os.path.relpath('/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/image', '$dirname'))"`
		cmdoptions=`echo $@ | sed -e "s::\\$realdir/$relpath:g"`
	fi
	cat <<END >$cmd
#!/bin/bash
realpath=\`readlink -fn \$0\`
realdir=\`dirname \$realpath\`
exec -a \`dirname \$realpath\`/$cmdname \`dirname \$realpath\`/$cmdname.real $cmdoptions "\$@"
END
	chmod +x $cmd
}

create_wrapper() {
	# Create a wrapper script where extra environment variables are needed
	#
	# These are useful to work around relocation issues, by setting environment
	# variables which point to paths in the filesystem.
	#
	# Usage: create_wrapper FILENAME [[VAR=VALUE]..]

	cmd=$1
	shift

	echo "Generating wrapper script for $cmd"

	mv $cmd $cmd.real
	cmdname=`basename $cmd`
	dirname=`dirname $cmd`
	exportstring=$@
	if [ "" != "" ]; then
		relpath=`python3 -c "import os; print(os.path.relpath('/home/vortex/build/Vortex_SourceCode/yocto/build_var-som-mx6/tmp/work/armv7at2hf-neon-fslc-linux-gnueabi/vortex-core/1.0.5-r0/image', '$dirname'))"`
		exportstring=`echo $@ | sed -e "s::\\$realdir/$relpath:g"`
	fi
	cat <<END >$cmd
#!/bin/bash
realpath=\`readlink -fn \$0\`
realdir=\`dirname \$realpath\`
export $exportstring
exec -a \`dirname \$realpath\`/$cmdname \`dirname \$realpath\`/$cmdname.real "\$@"
END
	chmod +x $cmd
}

die() {
	bbfatal_log "$*"
}

